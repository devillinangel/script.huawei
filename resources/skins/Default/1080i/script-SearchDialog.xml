<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">9000</defaultcontrol>
    <controls>
        <control type="image">
            <left>0</left>
            <top>0</top>
            <width>1920</width>
            <height>1080</height>
            <texture>views/search/search_bg.png</texture>
            <animation effect="fade" time="300">WindowOpen</animation>
            <animation effect="fade" time="300">WindowClose</animation>
        </control>
        <control type="group">
            <animation type="WindowOpen" reversible="false">
                <effect type="slide" start="0,80" end="0,0" delay="130" time="350" tween="cubic" />
                <effect type="fade" start="0" end="100" time="240" />
            </animation>
            <animation type="WindowClose" reversible="false">
                <effect type="slide" start="0,0" end="0,80" time="150" tween="cubic" easing="in" />
                <effect type="fade" start="100" end="0" time="240" />
            </animation>
            <control type="image">
                <description>Searchbox image</description>
                <left>184</left>
                <top>230</top>
                <width>530</width>
                <height>96</height>
                <texture>views/search/keyboard/search_box.png</texture>
            </control>
            <control type="label" id="600">
                <description>Searchbox label</description>
                <left>184</left>
                <top>230</top>
                <width>530</width>
                <height>96</height>
                <font>font36_shs_regular</font>
                <aligny>center</aligny>
                <textoffsetx>26</textoffsetx>
            </control>
